{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "default_icon": {
        "16": "/images/pcwee16.png",
        "32": "/images/pcwee32.png",
        "48": "/images/pcwee48.png",
        "128": "/images/pcwee128.png"
    },
    "icons": {
        "16": "/images/pcwee16.png",
        "32": "/images/pcwee32.png",
        "48": "/images/pcwee48.png",
        "128": "/images/pcwee128.png"
    }
}